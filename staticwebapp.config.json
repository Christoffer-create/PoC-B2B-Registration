{
    "routes": [
        {
            "route": "/*",
            "allowedRoles": ["authenticated"]
        }
    ],
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/246c62ec-5bd4-4fbd-918f-b132f3e239b1/v2.0"
                }
            }
        }
    },
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/aad"
        },
        "403": {
            "statusCode": 403,
            "redirect": "/unauthorized"
        }
    }
}
