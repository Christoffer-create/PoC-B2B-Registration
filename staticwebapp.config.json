{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    },
    "403": {
      "statusCode": 403,
      "redirect": "/unauthorized.html"
    }
  },
   "auth": {
     "identityProviders": {
       "azureActiveDirectory": {
         "registration": {
           "openIdIssuer": "https://login.microsoftonline.com/TENANT_ID/v2.0",
           "clientIdSettingName": "AZURE_CLIENT_ID"
         }
       }
     }
   }
  }
